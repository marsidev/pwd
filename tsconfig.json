{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "paths": {
      "~/*": ["src/*"],
      "@layout": ["src/layouts/Layout.astro"]
    }
  }
}
