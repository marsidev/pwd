---
import type { HTMLAttributes } from 'astro/types'
export type Props = HTMLAttributes<'div'>
const { props } = Astro
---

<div class='flex flex-col items-center justify-center' {...props}>
	<h1 class='text-xl sm:text-2xl font-bold text-center text-black'>Genere una contraseña ahora.</h1>

	<p class='text-lg sm:text-xl font-semibold text-black'>Y manténgase seguro en línea.</p>
</div>
